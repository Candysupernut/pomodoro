<script>
    function convertLength(from, value) {
        const inputValue = parseFloat(value);

        if (from === 'meters') {
            const centimeters = inputValue * 100;
            const millimeters = inputValue * 1000;
            const kilometers = inputValue / 1000;

            document.getElementById('outputCentimeters').value = centimeters.toFixed(2);
            document.getElementById('outputMillimeters').value = millimeters.toFixed(2);
            document.getElementById('outputKilometers').value = kilometers.toFixed(6);
            
            // Додаткові величини
            document.getElementById('outputFeet').value = (inputValue * 3.2808).toFixed(2);
            document.getElementById('outputYards').value = (inputValue * 1.0936).toFixed(2);
            document.getElementById('outputMiles').value = (inputValue * 0.00062137).toFixed(6);
            document.getElementById('outputInches').value = (inputValue * 39.370).toFixed(2);
        } else if (from === 'centimeters') {
            const meters = inputValue / 100;
            const millimeters = meters * 1000;
            const kilometers = meters / 1000;

            document.getElementById('inputMeters').value = meters.toFixed(2);
            document.getElementById('outputMillimeters').value = millimeters.toFixed(2);
            document.getElementById('outputKilometers').value = kilometers.toFixed(6);

            // Додаткові величини
            document.getElementById('outputFeet').value = (meters * 3.2808).toFixed(2);
            document.getElementById('outputYards').value = (meters * 1.0936).toFixed(2);
            document.getElementById('outputMiles').value = (meters * 0.00062137).toFixed(6);
            document.getElementById('outputInches').value = (meters * 39.370).toFixed(2);
        } else if (from === 'millimeters') {
            const meters = inputValue / 1000;
            const centimeters = meters * 100;
            const kilometers = meters / 1000;

            document.getElementById('inputMeters').value = meters.toFixed(2);
            document.getElementById('outputCentimeters').value = centimeters.toFixed(2);
            document.getElementById('outputKilometers').value = kilometers.toFixed(6);

            // Додаткові величини
            document.getElementById('outputFeet').value = (meters * 3.2808).toFixed(6);
            document.getElementById('outputYards').value = (meters * 1.0936).toFixed(6);
            document.getElementById('outputMiles').value = (meters * 0.00062137).toFixed(6);
            document.getElementById('outputInches').value = (meters * 39.370).toFixed(6);
        } else if (from === 'kilometers') {
            const meters = inputValue * 1000;
            const centimeters = meters * 100;
            const millimeters = meters * 1000;

            document.getElementById('inputMeters').value = meters.toFixed(2);
            document.getElementById('outputCentimeters').value = centimeters.toFixed(2);
            document.getElementById('outputMillimeters').value = millimeters.toFixed(2);

            // Додаткові величини
            document.getElementById('outputFeet').value = (meters * 3.2808).toFixed(2);
            document.getElementById('outputYards').value = (meters * 1.0936).toFixed(2);
            document.getElementById('outputMiles').value = (meters * 0.00062137).toFixed(6);
            document.getElementById('outputInches').value = (meters * 39.370).toFixed(2);
        } else if (from === 'feet') {
            const meters = inputValue / 3.2808;
            const centimeters = meters * 100;
            const millimeters = meters * 1000;
            const kilometers = meters / 1000;

            document.getElementById('inputMeters').value = meters.toFixed(2);
            document.getElementById('outputCentimeters').value = centimeters.toFixed(2);
            document.getElementById('outputMillimeters').value = millimeters.toFixed(2);
            document.getElementById('outputKilometers').value = kilometers.toFixed(6);

            // Додаткові величини
            // Виводимо значення вже введених величин у відповідних полях
            document.getElementById('outputYards').value = (meters * 1.0936).toFixed(2);
            document.getElementById('outputMiles').value = (meters * 0.00062137).toFixed(6);
            document.getElementById('outputInches').value = (meters * 39.370).toFixed(2);
        } else if (from === 'yards') {
            const meters = inputValue / 1.0936;
            const centimeters = meters * 100;
            const millimeters = meters * 1000;
            const kilometers = meters / 1000;

            document.getElementById('inputMeters').value = meters.toFixed(2);
            document.getElementById('outputCentimeters').value = centimeters.toFixed(2);
            document.getElementById('outputMillimeters').value = millimeters.toFixed(2);
            document.getElementById('outputKilometers').value = kilometers.toFixed(6);

            // Додаткові величини
            document.getElementById('outputFeet').value = (meters * 3.2808).toFixed(2);
            document.getElementById('outputMiles').value = (meters * 0.00062137).toFixed(6);
            document.getElementById('outputInches').value = (meters * 39.370).toFixed(2);
        } else if (from === 'miles') {
            const meters = inputValue / 0.00062137;
            const centimeters = meters * 100;
            const millimeters = meters * 1000;
            const kilometers = meters / 1000;

            document.getElementById('inputMeters').value = meters.toFixed(2);
            document.getElementById('outputCentimeters').value = centimeters.toFixed(2);
            document.getElementById('outputMillimeters').value = millimeters.toFixed(2);
            document.getElementById('outputKilometers').value = kilometers.toFixed(6);

            // Додаткові величини
            document.getElementById('outputFeet').value = (meters * 3.2808).toFixed(2);
            document.getElementById('outputYards').value = (meters * 1.0936).toFixed(2);
            document.getElementById('outputInches').value = (meters * 39.370).toFixed(2);
        } else if (from === 'inches') {
            const meters = inputValue / 39.370;
            const centimeters = meters * 100;
            const millimeters = meters * 1000;
            const kilometers = meters / 1000;

            document.getElementById('inputMeters').value = meters.toFixed(2);
            document.getElementById('outputCentimeters').value = centimeters.toFixed(2);
            document.getElementById('outputMillimeters').value = millimeters.toFixed(2);
            document.getElementById('outputKilometers').value = kilometers.toFixed(6);

            // Додаткові величини
            document.getElementById('outputFeet').value = (meters * 3.2808).toFixed(2);
            document.getElementById('outputYards').value = (meters * 1.0936).toFixed(2);
            document.getElementById('outputMiles').value = (meters * 0.00062137).toFixed(6);
        }
    }
</script>